{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api", "dest": "index.js" },
    { "src": "/create-payment-intent", "dest": "index.js" },
    { "src": "/(.*)", "dest": "index.js" }
  ]
}
